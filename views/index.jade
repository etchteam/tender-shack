extends layout

block content
  .container
    .row
      .col-xs-12
          each tender in tenders
            section.panel.panel-default
              .panel-heading
                h2.panel-title: a(href="#{tender.link}")!= tender.title
              .panel-body
                p.lead.text-uppercase.text-success: strong #{tender.value}
                p(style='white-space: pre-wrap') #{tender.description}
